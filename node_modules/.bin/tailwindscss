#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/quangnhatnguyen/Projects/Personal/blog/node_modules/.pnpm/tailwindscss@0.3.0/node_modules/tailwindscss/bin/node_modules:/Users/quangnhatnguyen/Projects/Personal/blog/node_modules/.pnpm/tailwindscss@0.3.0/node_modules/tailwindscss/node_modules:/Users/quangnhatnguyen/Projects/Personal/blog/node_modules/.pnpm/tailwindscss@0.3.0/node_modules:/Users/quangnhatnguyen/Projects/Personal/blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/quangnhatnguyen/Projects/Personal/blog/node_modules/.pnpm/tailwindscss@0.3.0/node_modules/tailwindscss/bin/node_modules:/Users/quangnhatnguyen/Projects/Personal/blog/node_modules/.pnpm/tailwindscss@0.3.0/node_modules/tailwindscss/node_modules:/Users/quangnhatnguyen/Projects/Personal/blog/node_modules/.pnpm/tailwindscss@0.3.0/node_modules:/Users/quangnhatnguyen/Projects/Personal/blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindscss/bin/cli.js" "$@"
else
  exec node  "$basedir/../tailwindscss/bin/cli.js" "$@"
fi
